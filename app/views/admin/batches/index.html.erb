<table class="table table-striped">
  <tr>
    <th>Class Name</th>
    <th>Subjects</th>
  </tr>
  <%@batches.each do |b|%>
    <tr>
      <td>
        <%=b.batch_name%>
      </td>
      <td>
        <% b.subjects.each do |s| %>
          <%= s.subject_name %> &nbsp;
        <%end%>
      </td>
    </tr>
  <%end%>
</table>
<%= link_to "New",new_admin_batch_path %>

